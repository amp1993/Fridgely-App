{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "productName": {
        "type": "string",
        "minLength": 3
      },
      "categoryName": {
        "type": "string",
        "enum": [
          "Fruits",
          "Vegetables",
          "Meat",
          "Seafood",
          "Eggs",
          "Milk",
          "Cheese",
          "Oil",
          "Cans",
          "Seasonings",
          "Cereals & Granola",
          "Condiments & Sauces",
          "Bakery & Bread",
          "Snacks",
          "Beverages",
          "Frozen Foods",
          "Delicatessen",
          "Misc"
        ]
      },
      "unitOfMeasure": {
        "enum": [null,"kg", "g", "lb", "oz", "liter", "ml", "pint", "quart", "gallon", "dozen", "piece"]
      },
      "username":{
        "type": "string"
      },
      "inGroceryList":{
        "type":"boolean"
      },
      "quantityInGroceryList": {
        "type": "integer"
      },
      "inFridge":{
        "type":"boolean"
      },
      "quantityInFridge": {
        "type": "integer"
      }
    },
    "additionalProperties": false,
    "required": [
      "productName",
      "categoryName",
      "username"
    ]
  }
  